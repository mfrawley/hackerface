{
  "manifest_version": 2,
  "name": "HackerFace",
  "description": "Hacker faces",
  "version": "2.0",
  "content_scripts": [
     {
       "matches": ["https://news.ycombinator.com/*"],
       "css": ["hackerface.css"],
       "js": ["hackerface.js"]
     }
   ],
  // "permissions": [
  //   "https://news.ycombinator.com/"
  // ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
